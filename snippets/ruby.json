{
  "frozen string": {
    "prefix": [ "frz" ],
    "body": [
      "# frozen_string_literal: true",
      ""
    ],
    "description": "frozen magic comment"
  },
  "binding.pry": {
    "prefix": [ "bp" ],
    "body": [
      "binding.pry"
    ],
    "description": "binding pry"
  },
  "factory bot": {
    "prefix": [ "factory_bot" ],
    "body": [
      "FactoryBot.define do",
      "  factory :hoge do",
      "  end",
      "end"
    ],
    "description": "rspec desctibe block"
  },
  "rspec describe": {
    "prefix": [ "desc" ],
    "body": [
      "describe \"$0\" do",
      "end"
    ],
    "description": "rspec desctibe block"
  },
  "rspec context": {
    "prefix": [ "cntx" ],
    "body": [
      "context \'$0\' do",
      "end"
    ],
    "description": "rspec context block"
  },
  "rspec subject": {
    "prefix": [ "subj" ],
    "body": [
      "subject { $0 }"
    ],
    "description": "rspec subject { ~ }"
  },
  "rspec it {}": {
    "prefix": [ "it{}" ],
    "body": [
      "it { $0 }"
    ],
    "description": "it { ~ }"
  },
  "rspec it block": {
    "prefix": [ "itdo" ],
    "body": [
      "it do",
      "  $0",
      "end"
    ],
    "description": "rspec it block"
  },
  "rspec expect().to": {
    "prefix": [ "expect()" ],
    "body": [
      "expect($0).to $1"
    ],
    "description": "expect().to"
  },
  "rspec it is_expected {}": {
    "prefix": [ "itis" ],
    "body": [
      "it { is_expected.to $0 }"
    ],
    "description": "rspec it { is_expected }"
  },
  "rspec is_expected {}": {
    "prefix": [ "is_expected" ],
    "body": [
      "is_expected.to $0"
    ],
    "description": "rspec it { is_expected }"
  },
  "rspec change{}.by": {
    "prefix": [ "change{}.by" ],
    "body": [
      "change { $0 }.by($1)"
    ],
    "description": "rspec change_by"
  },
  "rspec change{}.from_to": {
    "prefix": [ "change{}.from_to" ],
    "body": [
      "change { $0 }.from($1).to($2)"
    ],
    "description": "rspec change_from_to"
  }
}
