	email = 30695524+uhsog@users.noreply.github.com
[core]
	editor = vim -c \"set fenc=utf-8\"
[color]
	diff = auto
	status = auto
	branch = auto
[alias]
  s = status
  stt = status -uno     # Untracked filesを表示せず，not stagedと，stagedだけの状態を出力する
  l = log --graph
  le = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
  b = branch
  d = diff
  co = checkout
  cm = commit -uno
  ft = fetch
  sw = switch
  swc = switch -c
[pager]
  # log = diff-highlight | less
  show = diff-highlight | less
  diff = diff-highlight | less
